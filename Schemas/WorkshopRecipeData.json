{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "id": "https://github.com/JamesVeug/ATS_JSONLoader",
  "type": "object",
  "title": "Workshop Recipe",
  "description": "Recipes added to workshop buildings to produce goods",
  "required": [
    "guid",
    "name"
  ],
  "properties": {
    "guid": {
      "description": "Unique identifier for the mod that added this. Blank if it's added as part of the base game.",
      "pattern": "^[a-zA-Z\\d]+$",
      "type": "string"
    },
    "name": {
      "description": "Unique identifier",
      "pattern": "^[a-zA-Z\\d_]+$",
      "minLength": 1,
      "type": "string"
    },
    "grade": {
      "default": "One",
      "description": "The grade of the recipe",
      "anyOf": [
        {
          "type": "string",
          "title": "Predefined Grade",
          "enum": [
            "Zero",
            "One",
            "Two",
            "Three"
          ]
        },
        {
          "type": "string",
          "title": "Mod-Added Grade"
        }
      ]
    },
    "producedGood": {
      "default": "Crafting_Oil",
      "description": "The produced good when crafted",
      "anyOf": [
        {
          "type": "string",
          "title": "Predefined GoodsTypes",
          "enum": [
            "_Meta_Artifacts",
            "_Meta_Food_Stockpiles",
            "_Meta_Machinery",
            "Blight_Fuel",
            "Crafting_Coal",
